<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"><![CDATA[
    android:paddingTop="5dp"
    android:paddingBottom="5dp">


    ]]>

    <TextView
        android:id="@+id/text_list_name"
        android:layout_width="157dp"
        android:layout_height="29dp"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_list_firstname"
        android:layout_width="176dp"
        android:layout_height="25dp"
        android:text="Firstname"
        app:layout_constraintBottom_toBottomOf="@+id/text_list_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toEndOf="@+id/text_list_name"
        app:layout_constraintTop_toTopOf="@+id/text_list_name" />
</androidx.constraintlayout.widget.ConstraintLayout>